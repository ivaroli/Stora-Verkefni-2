@model List<BookApp.Models.Book>
@{
    ViewData["Title"] = "Books";
}
<div class="container" id="">
    <div class="tree"><a href="#">Home page</a> >> <a href="#">Books</a></div>
 <h1>BOOKS</h1>
 <ul>
@foreach(var book in Model)
{
<li>
Title: @book.Title <br/>
Author: @book.Author.Name <br/>
Genre: @book.Genre <br/>
Description: @book.Description <br/>
@book.Image <br/>
</li>
<br/>
}
</ul>

 </div>